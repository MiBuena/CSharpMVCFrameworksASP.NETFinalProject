@model IEnumerable<LeisureTimeSystem.Models.ViewModels.Profile.StudentProfileOrganizationViewModel>


<p>Your organizations</p>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.City)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Telephones)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.City)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Telephones)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", "Organization", new { organizationId=item.Id }, null)
        </td>
    </tr>
}

</table>
