@model LeisureTimeSystem.Models.ViewModels.Organization.EditOrganizationPicturesViewModel


<div class="left presentation_gallery_photo">
    <table width="100" style="width: 100%; height: 103px;">
        <tbody>
        <tr>
            @foreach (var picture in this.Model.Pictures)
            {
                <td width="100" height="100" valign="middle" align="center" style="vertical-align: middle;">
                    @Html.ActionLink("Delete", "DeletePicture", "Organization", new {pictureId = picture.Id, organizationId = this.Model.OrganizationId}, null)
                    <img alt="Детска Академия по Футбол - снимка 1 - футбол - София" src=@picture.Path>
                </td>
            }
        </tr>
        </tbody>
    </table>
</div>



@Html.Action("UploadFile", new {organizationId = this.Model.OrganizationId})


<div>
    @Html.ActionLink("Back to List", "Index")
</div>
